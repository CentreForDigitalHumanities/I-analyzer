{
  "name": "frontend",
  "scripts": {
    "ng": "ng",
    "start": "yarn serve",
    "start-docker": "ng serve --host 0.0.0.0 --port 4200 --proxy-config proxy.conf.docker.json",
    "preserve": "yarn prebuild",
    "serve": "ng serve --proxy-config proxy.conf.json",
    "prebuild": "node ./build/build-pre.js",
    "build": "ng build --configuration=production --base-href=/static/",
    "build-local": "ng build --aot",
    "build-git": "ng build --configuration git --aot",
    "test:debug": "ng test",
    "pretest": "yarn prebuild",
    "test": "ng test --source-map=false --watch=false",
    "lint": "ng lint",
    "stop": "lsof -t -i tcp:4200 | xargs kill -9 || echo \"not running\"",
    "watch": "ng build --configuration=production --watch"
  },
  "private": true,
