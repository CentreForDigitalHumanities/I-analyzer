{
    "name": "i-analyzer",
    "version": "0.0.0",
    "license": "MIT",
    "pre-commit": {
        "run": [
            "test-once"
        ]
    },
    "scripts": {
        "postinstall": "npm run install-api && npm run install-web-ui",
        "install-api": "cd api && pip install -r requirements.txt",
        "install-web-ui": "cd web-ui && npm install",
        "test-once": "cd web-ui && ng test --watch=false"
    },
    "private": true,
    "devDependencies": {
        "pre-commit": "^1.2.2"
    }
}
